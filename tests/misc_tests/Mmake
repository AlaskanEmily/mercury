#-----------------------------------------------------------------------------#

main_target: check

include ../Mmake.common

#-----------------------------------------------------------------------------#

mdemangle_test.out: mdemangle_test.inp
	mdemangle < mdemangle_test.inp > mdemangle_test.out

#-----------------------------------------------------------------------------#

PROGS=	

#-----------------------------------------------------------------------------#

DEPS=	$(PROGS:%=%.dep)
DEPENDS=$(PROGS:%=%.depend)
OUTS=	$(PROGS:%=%.out) mdemangle_test.out
RESS=	$(PROGS:%=%.res) mdemangle_test.res
MODS=	$(PROGS:%=%.mod)

#-----------------------------------------------------------------------------#

dep:	$(DEPS)

depend:	$(DEPENDS)

check:	$(OUTS) $(RESS)

mods:	$(MODS)

all:	$(PROGS)

#-----------------------------------------------------------------------------#
