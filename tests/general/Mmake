#-----------------------------------------------------------------------------#

main_target: check

include ../Mmake

#-----------------------------------------------------------------------------#

PROGS=	arithmetic string_test string_test_2 interpreter disj_disj \
	nondet_disj partition nasty_nondet
# We do not yet pass the following tests:
#	petdr1

DEPENDS=$(PROGS:%=%.depend)
OUTS=	$(PROGS:%=%.out)
EXPS=	$(PROGS:%=%.exp)
RESS=	$(PROGS:%=%.res)
MODS=	$(PROGS:%=%.mod)

depend: $(DEPENDS)

check: $(EXPS) $(OUTS) $(RESS)

mods: $(MODS)

all:	$(PROGS)

#-----------------------------------------------------------------------------#
